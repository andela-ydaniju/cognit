<div class="row">
  <div class="col-xs-12">
    <div class="row center-xs middle-xs" style="">
      <div class="col-xs-12">
        <div class="box">
          <div class="row center-xs" style="margin-top: 8%; background-color: white ">
            <div class="col-xs-8">
              <%= @result.each do |x| %>
              <div class="col-xs border">
                <h1><%= x[:name] %></h1>
                <div class="col-xs-8">
                  <div class="progress">
                    <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: <%= x[:values][:Openness] * 100 %>%">
                      Openness <%= x[:values][:Openness] * 100 %>
                    </div>
                  </div>
                  <div class="progress">
                    <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: <%= x[:values][:Conscientiousness] * 100 %>%">
                      Conscientiousness <%= x[:values][:Conscientiousness] * 100 %>
                    </div>
                  </div>
                  <div class="progress">
                    <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: <%= x[:values][:Extraversion] * 100 %>%">
                      Extraversion <%= x[:values][:Extraversion] * 100 %>
                    </div>
                  </div>
                  <div class="progress">
                    <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: <%= x[:values][:Agreeableness] * 100 %>%">
                      Agreeableness <%= x[:values][:Agreeableness] * 100 %>
                    </div>
                  </div>
                </div>
              </div>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>